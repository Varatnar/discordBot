  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": false,
    "removeComments": true,
    "strictPropertyInitialization": false,
    "sourceMap": false,
    "noImplicitAny": true,
    "noImplicitThis": true
  },
  "include": [
    "src/**/*"
  ],  "exclude": [
    "node_modules",
    "**/*.sepc.ts"
  ]
}